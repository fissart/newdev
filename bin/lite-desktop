    cp $desktoppath/computer.desktop /home/$USER/डेस्कटॉप 2>/dev/null
fi

echo $desktopicons | grep "This PC - Enable" > /dev/null
if [ $? = 0 ];then
    cp $desktoppath/computer.desktop /home/$USER/"Radna površina" 2>/dev/null
fi

echo $desktopicons | grep "This PC - Enable" > /dev/null
if [ $? = 0 ];then
    cp $desktoppath/computer.desktop /home/$USER/Scrivania 2>/dev/null
