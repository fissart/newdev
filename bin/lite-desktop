#! /bin/bash
#--------------------------------------------------------------------------------------------------------
# Name: Linux Lite - Lite Desktop
# Add/Remove Desktop icons in Linux Lite
# Architecture: all
# Author: Jerry Bezencon
# Website: https://www.linuxliteos.com
#--------------------------------------------------------------------------------------------------------

# Set variables
desktoppath="/usr/share/litedesktop/"
ic="/usr/share/icons/Papirus/24x24/apps/litedesktop.png"		# Lite Desktop icon variable

desktopicons=$(yad --borders=20 --window-icon="$ic" --text-align="center" --list --title "Desktop Icons" --text "Select the Desktop Icon or Icons\nthat you wish to <b>Enable</b> or <b>Disable</b>\n" --checklist --width="460" --height="510" \
--column "Select" --column " " --column "Desktop Icon" \
FALSE " " " This PC - Enable" \
FALSE " " " User Files - Enable" \
FALSE " " " Network - Enable" \
FALSE " " " Control Panel - Enable" \
FALSE " " " Help Manual - Enable" \
FALSE " " " This PC - Disable" \
FALSE " " " User Files - Disable" \
FALSE " " " Network - Disable" \
FALSE " " " Control Panel - Disable" \
FALSE " " " Help Manual - Disable" \
--separator=",");

# Enable Desktop Icon/s

# This PC - Enable
echo $desktopicons | grep "This PC - Enable" > /dev/null
if [ $? = 0 ];then
    cp $desktoppath/computer.desktop /home/$USER/Desktop 2>/dev/null
fi

echo $desktopicons | grep "This PC - Enable" > /dev/null
if [ $? = 0 ];then
    cp $desktoppath/computer.desktop /home/$USER/Werkskerm 2>/dev/null
fi

echo $desktopicons | grep "This PC - Enable" > /dev/null
if [ $? = 0 ];then
    cp $desktoppath/computer.desktop /home/$USER/"سطح المكتب"
fi

echo $desktopicons | grep "This PC - Enable" > /dev/null
if [ $? = 0 ];then
    cp $desktoppath/computer.desktop /home/$USER/Schreibtisch 2>/dev/null
fi

